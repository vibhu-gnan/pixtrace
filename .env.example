# Database (Supabase PostgreSQL)
DATABASE_URL="postgresql://user:password@db.supabase.co:5432/postgres"

# Supabase Auth
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Cloudflare R2 (S3-compatible)
R2_ACCOUNT_ID="your-account-id"
R2_ACCESS_KEY_ID="your-access-key"
R2_SECRET_ACCESS_KEY="your-secret-key"
R2_BUCKET_NAME="pixtrace-media"
R2_PUBLIC_URL="https://pub-xxxxx.r2.dev"

# Cloudflare Images
CLOUDFLARE_ACCOUNT_HASH="your-account-hash"
CLOUDFLARE_IMAGES_DELIVERY_URL="https://imagedelivery.net/your-hash"

# AWS SageMaker (for AI embeddings)
SAGEMAKER_ENDPOINT_URL="https://runtime.sagemaker.us-east-1.amazonaws.com"
SAGEMAKER_ENDPOINT_NAME="arcface-embedding-endpoint"
AWS_ACCESS_KEY_ID="your-aws-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret"

# Next.js
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"
